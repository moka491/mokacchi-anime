{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "concurrently \"npm run dev:tailwind\" \"npm run dev:netlify\"",
    "dev:tailwind": "tailwindcss -o ./app/tailwind.css --watch",
    "dev:netlify": "cross-env NODE_ENV=development netlify dev",
    "build": "npm run build:tailwind && remix build",
    "build:tailwind": "tailwindcss -o ./app/tailwind.css",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "lodash.throttle": "^4.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.33.1",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/lodash.throttle": "^4.1.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
